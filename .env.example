# Copy this file to .env and fill in the values
# Used by DeepAgents CLI and examples

# Required for running agents (Anthropic LLM)
ANTHROPIC_API_KEY=your_anthropic_key_here

# Required for web search tool
TAVILY_API_KEY=your_tavily_key_here

# Optional: Arize/Phoenix tracing (recommended)
ARIZE_API_KEY=your_arize_api_key_here
ARIZE_SPACE_ID=your_arize_space_id_here

# Optional: Phoenix evaluators (OpenAI as judge)
# Only needed if you run examples/research/eval_run.py with --run-evals
OPENAI_API_KEY=your_openai_key_here
