# Copy this file to .env and fill in the values
# Used by DeepAgents CLI and examples

# Required for running agents (Anthropic LLM)
ANTHROPIC_API_KEY=your_anthropic_key_here

# Required for web search tool
TAVILY_API_KEY=your_tavily_key_here

# Optional: Arize/Phoenix tracing (recommended)
ARIZE_API_KEY=your_arize_api_key_here
ARIZE_SPACE_ID=your_arize_space_id_here

# Optional: LLM pricing for cost attribution in spans (USD per 1K tokens)
# Used by tracing to compute llm.cost_usd
LLM_PRICE_INPUT_PER_1K=3.00
LLM_PRICE_OUTPUT_PER_1K=15.00

# Optional: Phoenix evaluators (OpenAI as judge)
# Only needed if you run examples/research/eval_run.py with --run-evals
OPENAI_API_KEY=your_openai_key_here

# Optional: local post-run evals (used by examples/research/dump_run.py)
# Set to 1/true to run evals automatically after a dump
# DEEPAGENTS_RUN_EVALS=1
# Choose evaluator model (overrides default)
# DEEPAGENTS_EVAL_MODEL=gpt-5-mini
